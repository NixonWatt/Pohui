@{
    ViewBag.Title = "Home Page";
}
<div class="container-fluid">
    <div class="row-fluid">
        <div class="col-lg-8">
        </div>
        <div class="col-lg-4">
            @Html.Partial("_Diagram")
            <div id="Tag"></div>
        </div>
    </div>
</div>

<script type="text/javascript">
        $.ajax({
            url: '/Creative/TagCloud',
            success: function (response) {
                $("#Tag").html(response);
            }
        });
</script>
